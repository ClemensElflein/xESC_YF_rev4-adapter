<library>
  <extends>project.xml</extends>
  
  <options>
    <!-- Override build path for HW V2 -->
    <option name="modm:build:build.path">../build/v2</option>
  </options>
  
  <collectors>
    <!-- Hardware variant specific C/C++ defines -->
    <collect name="modm:build:cppdefines">HW_V2=1</collect>
  </collectors>
</library>
